{
    "name": "luceos/data-breach",
    "description": "Gathers information to report data breach.",
    "keywords": [
        "flarum", "luceos", "extension"
    ],
    "license": "mit",
    "type": "flarum-extension",
    "authors": [
        {
            "name": "DaniÃ«l Klabbers",
            "email": "daniel@klabbers.email",
            "homepage": "https://luceos.com"
        }
    ],
    "require": {
        "php": ">= 8.2",
        "flarum/core": "^1.6.1",
        "fof/geoip": "*"
    },
    "autoload": {
        "psr-4": {
            "Luceos\\Breached\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Blomstra\\Breached\\Tests\\": "tests/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Breached",
            "category": "feature",
            "icon": {
                "image": "resources/assets/logo.png",
                "backgroundColor": "#E3E7F1",
                "backgroundSize": "60%",
                "backgroundRepeat": "no-repeat",
                "backgroundPosition": "center"
            }
        },
        "extiverse": {
            "discuss": "https://discuss.flarum.org/d/?"
        }
    }
}
